  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint src",
    "prepack": "yarn build",
    "prepublishOnly": "yarn lint && yarn test",
    "start": "ts-node src/bin/vizdiff.ts",
    "test": "yarn run vitest --run",
